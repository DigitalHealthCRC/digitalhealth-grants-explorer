{
"search_criteria": {
    "keywords": ["research grant", "innovation funding", "nonprofit grant"],
    "exclude_keywords": ["expired", "closed"],
    "funding_sources": [
        "https://www.health.gov.au/our-work/mrff/grant-opportunities-calendar",
        "https://www.nsf.gov/funding/"]},
"csv_settings": {
    "output_file": "grants_database.csv",
    "columns": [
"grant_id",
"title",
"agency",
"funding_amount",
"deadline",
"eligibility",
"description",
"url",
"discovered_date",
"hash_key"]},
"deduplication": {
    "key_fields": ["title", "agency", "deadline"],
    "similarity_threshold": 0.85
},
"schedule": {
    "day": 1,
    "hour": 9,
    "minute": 0}
}
